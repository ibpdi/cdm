            sh:maxLength 75 ;
            sh:minLength 1 ;
            sh:name "SimulationVersion" ;
            sh:path :hasSimulationVersion ;
            sh:pattern "[^<>[]{}|/()°\"&=+§^~:;$%!?0-cZ]+$" ],
        [ a sh:PropertyShape ;
