:SustainabilityIndicator a sh:NodeShape ;
    sh:property [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:description "Unit of sustainability indicator value" ;
            sh:in ( "kgCO2e" "kgCO2e/sqm" "kWh" "kWh/sqm" "EUR" "sqm" ) ;
            sh:maxCount 1 ;
            sh:maxLength 50 ;
            sh:minLength 1 ;
            sh:name "SustainabilityIndicatorUnit" ;
            sh:path :hasSustainabilityIndicatorUnit ;
            sh:pattern "[^<>[]{}|()°\"&=+§^~:;$%!?0-cZ]+$" ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:description "Date validity starts in yyyy-mm-ddThh:mm:ssZ form (conform to ISO 8061)" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "ValidFrom" ;
            sh:path :hasValidFrom ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:description "Type of sustainability indicator e.g. absolute or intensity figures or stranding" ;
            sh:in ( "Absolute" "Intensity" "Excess emissions" "Stranding" "Target" "Budget" "Direct emissions" "Indirect emissions" ) ;
            sh:maxCount 1 ;
            sh:maxLength 50 ;
            sh:minLength 1 ;
            sh:name "SustainabilityIndicatorType" ;
            sh:path :hasSustainabilityIndicatorType ;
            sh:pattern "[^<>[]{}|/()°\"&=+§^~:;$%!?0-cZ]+$" ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:description "Date validity ends in yyyy-mm-ddThh:mm:ssZ form (conform to ISO 8061)" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "ValidUntil" ;
            sh:path :hasValidUntil ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:description "Approach of reporting emissions e.g. from 'green electricity consumption': Market-based or location-based" ;
            sh:in ( "Location-based" "Market-based" ) ;
            sh:maxCount 1 ;
            sh:maxLength 50 ;
            sh:minLength 1 ;
            sh:name "ReportingType" ;
            sh:path :hasReportingType ;
            sh:pattern "[^<>[]{}|/()°\"&=+§^~:;$%!?0-cZ]+$" ],
        [ a sh:PropertyShape ;
            sh:description "Identifier of building" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "BuildingId" ;
            sh:node :BuildingId ;
            sh:path base:hasForeignKey ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:description "Overall category of sustainability indicator e.g. energy, greenhouse gas emissions or costs" ;
            sh:in ( "Costs" "Energy" "Ghg" "Area" ) ;
            sh:maxCount 1 ;
            sh:maxLength 50 ;
            sh:minCount 1 ;
            sh:minLength 1 ;
            sh:name "Category" ;
            sh:path :hasCategory ;
            sh:pattern "[^<>[]{}|/()°\"&=+§^~:;$%!?0-cZ]+$" ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:decimal ;
            sh:description "Value of sustainability indicator" ;
            sh:maxCount 1 ;
            sh:name "Value" ;
            sh:path :hasValue ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:description "Base year of values projected into the future in yyyy-mm-ddThh:mm:ssZ form (conform to ISO 8061). In case no exact date is used please use xxxx-01-01 00:00:00." ;
            sh:maxCount 1 ;
            sh:name "BaseYear" ;
            sh:path :hasBaseYear ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:description "Operational source of energy or emission figures e.g. electricity or water" ;
            sh:in ( "Energy" "All" "Fuels" "Electricity" "District heating" "District cooling" "Fugitive" "Water supply" "Water discharge" "Water" "Waste" "District heating & fuel" "F-gases" ) ;
            sh:maxCount 1 ;
            sh:maxLength 50 ;
            sh:minLength 1 ;
            sh:name "Source" ;
            sh:path :hasSource ;
            sh:pattern "[^<>[]{}|/()°\"&=+§^~:;$%!?0-cZ]+$" ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:description "Reference area of building used as denominator in intensity calculation" ;
            sh:in ( "Net rentable area" "Gross internal area" ) ;
            sh:maxCount 1 ;
            sh:maxLength 50 ;
            sh:minLength 1 ;
            sh:name "ReferenceArea" ;
            sh:path :hasReferenceArea ;
            sh:pattern "[^<>[]{}|/()°\"&=+§^~:;$%!?0-cZ]+$" ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:description "Unique identifier either coming from previous system otherwise it needs to be define" ;
            sh:maxCount 1 ;
            sh:maxLength 36 ;
            sh:minCount 1 ;
            sh:minLength 36 ;
            sh:name "SustainabilityIndicatorId" ;
            sh:path :hasSustainabilityIndicatorId ;
            sh:pattern "^[0-9A-Fa-f]{8}(?:-[0-9A-Fa-f]{4}){3}-[0-9A-Fa-f]{12}$" ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:description "Subtype of sustainability indicator e.g. net-energy or  emission scope" ;
            sh:maxCount 1 ;
            sh:maxLength 50 ;
            sh:minLength 1 ;
            sh:name "SustainabilityIndicatorSubType" ;
            sh:path :hasSustainabilityIndicatorSubType ;
            sh:pattern "[^<>[]{}|/()°\"&=+§^~:;$%!?0-cZ]+$" ] .

:SustainabilityIndicatorEmissionFactor a sh:NodeShape ;
    sh:property [ a sh:PropertyShape ;
            sh:description "Unique identifier either coming from previous system otherwise it needs to be define" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "EmissionFactorId" ;
            sh:node :EmissionFactorId ;
            sh:path base:hasForeignKey ],
        [ a sh:PropertyShape ;
            sh:description "Unique identifier either coming from previous system otherwise it needs to be define" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "SustainabilityIndicatorId" ;
            sh:node :SustainabilityIndicatorId ;
            sh:path base:hasForeignKey ] .

:GhgEmissionId a sh:NodeShape ;
    sh:property [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:name "GhgEmissionId" ;
            sh:path :hasGhgEmissionId ] .

:EmissionFactorId a sh:NodeShape ;
    sh:property [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:name "EmissionFactorId" ;
            sh:path :hasEmissionFactorId ] .

:OperationalMeasurementId a sh:NodeShape ;
    sh:property [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:name "OperationalMeasurementId" ;
            sh:path :hasOperationalMeasurementId ] .

