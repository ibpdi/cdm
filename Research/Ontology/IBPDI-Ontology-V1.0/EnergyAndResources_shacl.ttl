            sh:path :hasCoordinateSystem ;
            sh:pattern "[^<>[]{}|/()°\"&=+§^~:;$%!?0-cZ]+$" ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:description "Information on members of a model ensemble and changed conditions ('realisation', 'initialisation', 'physics'). Control member; 'r1i1p1'" ;
            sh:maxCount 1 ;
            sh:maxLength 75 ;
            sh:minLength 1 ;
            sh:name "EnsembleMember" ;
            sh:path :hasEnsembleMember ;
            sh:pattern "[^<>[]{}|/()°\"&=+§^~:;$%!?0-cZ]+$" ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:description "Temporal resolution of data (can be the time period over which raw data is aggregated)" ;
            sh:maxCount 1 ;
            sh:maxLength 50 ;
            sh:minLength 1 ;
            sh:name "TemporalResolution" ;
            sh:path :hasTemporalResolution ;
            sh:pattern "[^<>[]{}|/()°\"&=+§^~:;$%!?0-cZ]+$" ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:description "Date observation ends in yyyy-mm-ddThh:mm:ssZ form (conform to ISO 8061)" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "ValidUntil" ;
            sh:path :hasValidUntil ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:decimal ;
            sh:description "Value of observed variable" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "Value" ;
            sh:path :hasValue ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:description "Unit of observed value" ;
            sh:maxCount 1 ;
            sh:maxLength 50 ;
            sh:minCount 1 ;
            sh:minLength 1 ;
            sh:name "ClimateUnit" ;
            sh:path :hasClimateUnit ;
            sh:pattern "[^<>[]{}|/()\"&=+§^~:;$%!?0-cZ]+$" ] .

