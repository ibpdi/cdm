: a owl:Ontology .

:Climate a sh:NodeShape ;
    sh:property [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:description "Type of simulation of regional climate model e.g. Historical, Evaluation, RCP2.6" ;
            sh:maxCount 1 ;
            sh:maxLength 75 ;
            sh:minLength 1 ;
            sh:name "Experiment" ;
            sh:path :hasExperiment ;
            sh:pattern "[^<>[]{}|/()°\"&=+§^~:;$%!?0-cZ]+$" ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:description "Spatial resolution of data" ;
            sh:maxCount 1 ;
            sh:maxLength 50 ;
            sh:minLength 1 ;
            sh:name "SpatialResolution" ;
            sh:path :hasSpatialResolution ;
            sh:pattern "[^<>[]{}|/()°\"&=+§^~:;$%!?0-cZ]+$" ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:description "Localisation of observed data in case of non-grid location types" ;
            sh:maxCount 1 ;
            sh:maxLength 50 ;
            sh:minLength 1 ;
            sh:name "Location" ;
            sh:path :hasLocation ;
            sh:pattern "[^<>[]{}|/()°\"&=+§^~:;$%!?0-cZ]+$" ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:decimal ;
            sh:description "Latitude coordinate in case of geographic coordinates" ;
            sh:maxCount 1 ;
            sh:name "Latitude" ;
            sh:path :hasLatitude ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:description "Date observation starts in yyyy-mm-ddThh:mm:ssZ form (conform to ISO 8061)" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "ValidFrom" ;
            sh:path :hasValidFrom ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:description "Observed variable" ;
            sh:maxCount 1 ;
            sh:maxLength 50 ;
            sh:minCount 1 ;
            sh:minLength 1 ;
            sh:name "Variable" ;
            sh:path :hasVariable ;
            sh:pattern "[^<>[]{}|/()°\"&=+§^~:;$%!?0-cZ]+$" ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:integer ;
            sh:description "Vertical coordinate in case of projected coordinates" ;
            sh:maxCount 1 ;
            sh:name "PosY" ;
            sh:path :hasPosY ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:decimal ;
            sh:description "Longitude coordinate in case of geographic coordinates" ;
            sh:maxCount 1 ;
            sh:name "Longitude" ;
            sh:path :hasLongitude ],
        [ a sh:PropertyShape ;
